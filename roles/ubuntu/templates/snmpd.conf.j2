# define RO community
rocommunity     # define RO community
rocommunity     t0k0p3d14
com2sec notConfigUser  default       t0k0p3d14
group   notConfigGroup v1           notConfigUser
group   notConfigGroup v2c           notConfigUser

view    systemview     included      system
access  notConfigGroup ""      any       noauth    exact  systemview none none
com2sec local     localhost       t0k0p3d14
com2sec local     127.0.0.0/8     t0k0p3d14
com2sec mynetwork 192.168.1.0/24      t0k0p3d14
group MyRWGroup  any        local
group MyROGroup  any        mynetwork

view all    included  .1                               80
access MyROGroup ""      any       noauth    0      all    none   none
access MyRWGroup ""      any       noauth    0      all    all    all

sysdescr        "Tokopedia Server"
syscontact      <IT@tokopedia.com> 
sysname         Tokopedia HQ
syslocation     Kebon Jeruk JAKARTA

proc mountd

exec echotest /bin/echo hello world

disk / 10000
disk /data 10000

load 12 14 14
pass .1.3.6.1.4.1.4413.4.1 /usr/bin/ucd5820stat
